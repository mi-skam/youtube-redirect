{
  "manifest_version": 2,
  "name": "youtube-redirect",
  "description": "redirects to youtube's subscription feed",
  "version": "1.0.0",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": ["background_script.js"]
  },
  "permissions": [
    "sessions",
    "webRequest",
    "webRequestBlocking",
    "*://*.youtube.com/"
  ]
}
